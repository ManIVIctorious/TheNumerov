   - z[a][5] * z[b][2] * q[3] * q[0] * d2dq2[2][3]
   + z[a][5] * z[b][2] * q[3] * q[3] * d2dq2[0][2]
   - z[a][5] * z[b][3] * q[3] * q[1] * d2dq2[2][2]
   - z[a][5] * z[b][4] * q[3] * q[1] * d2dq2[2][3]
   + z[a][5] * z[b][4] * q[3] * q[3] * d2dq2[1][2]
   + z[a][5] * z[b][5] * q[3] * q[3] * d2dq2[2][2]
}
